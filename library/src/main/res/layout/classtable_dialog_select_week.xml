<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/classtable_dialog_select_week_linear"
    android:orientation="vertical" android:layout_width="match_parent"
    android:clickable="true"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/classtable_class_page_toolbar_height">
        <com.ylq.library.classtable.widget.SelectWeekLayout
            android:id="@+id/classtable_select_week_seleclayout"
            android:background="@color/alpha_white"
            android:layout_width="@dimen/classtable_dialog_select_week_listview_width"
            android:layout_gravity="center"
            android:layout_height="@dimen/classtable_dialog_select_week_listview_height"
            android:padding="6dp">
            <ListView
                android:id="@+id/classtable_dialog_select_week_listview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scrollbars="none"
                android:paddingBottom="10dp"
                android:layout_centerHorizontal="true"/>
        </com.ylq.library.classtable.widget.SelectWeekLayout>

    </LinearLayout>

</LinearLayout>